{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/tab-screens": [
        "./app/_tab-screens/index"
      ],
      "@/tab-screens/*": [
        "./app/_tab-screens/*"
      ],
      "@/secondary-screens": [
        "./app/_secondary-screens/index"
      ],
      "@/secondary-screens/*": [
        "./app/_secondary-screens/*"
      ],
      "@/secondary-routes-config": [
        "./app/_secondary-screens/_map-secondary-routes-to-view.config.tsx"
      ],
      "@/tab-routes-config": [
        "./app/_tab-screens/_map-tab-routes-to-view.config.tsx"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
